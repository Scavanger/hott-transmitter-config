<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="helicopterModel" type="helicopterModel"/>

  <xs:element name="wingedModel" type="wingedModel"/>

  <xs:complexType name="wingedModel">
    <xs:complexContent>
      <xs:extension base="baseModel">
        <xs:sequence>
          <xs:element name="aileronFlapType" type="aileronFlapType" minOccurs="0"/>
          <xs:element name="brakeInputChannel" type="xs:IDREF" minOccurs="0"/>
          <xs:element name="brakeOffset" type="xs:int"/>
          <xs:element name="channel8Delay" type="xs:boolean"/>
          <xs:element name="motorOnC1Type" type="motorOnC1Type" minOccurs="0"/>
          <xs:element name="profitrims" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="profiTrim" type="wingedProfiTrim" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="profiTrimSwitch" type="xs:IDREF" minOccurs="0"/>
          <xs:element name="tailType" type="tailType" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="baseModel">
    <xs:sequence>
      <xs:element name="appVersion" type="xs:int"/>
      <xs:element name="autoTimerReset" type="xs:boolean"/>
      <xs:element name="autoTrimSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="bound" type="xs:boolean"/>
      <xs:element name="channels" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="channel" type="channel" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="channelMappings" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="channelMapping" type="channelMapping" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="channelSequencer" type="channelSequencer" minOccurs="0"/>
      <xs:element name="clocks" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="clock" type="clock" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="controlSwitches" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="controlSwitch" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="dscOutputType" type="dscOutputType" minOccurs="0"/>
      <xs:element name="dualMixers" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="dualMixer" type="dualMixer" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="extPpmType" type="extPPMType" minOccurs="0"/>
      <xs:element name="failSafeDelay" type="xs:double"/>
      <xs:element name="failSafeSettingCheck" type="xs:boolean"/>
      <xs:element name="freeMixers" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="freeMixer" type="freeMixer" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="logicalSwitches" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="logicalSwitch" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="memoryVersion" type="xs:int"/>
      <xs:element name="modelName" type="xs:string" minOccurs="0"/>
      <xs:element name="modelNumber" type="xs:int"/>
      <xs:element name="modelType" type="modelType" minOccurs="0"/>
      <xs:element name="module" type="hfModule" minOccurs="0"/>
      <xs:element name="mp3Player" type="mp3Player" minOccurs="0"/>
      <xs:element name="multichannels" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="multichannel" type="multichannel" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="phases" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="phase" type="phase" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="phaseAssignment" type="phaseAssignment" minOccurs="0"/>
      <xs:element name="receivers" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="receiver" type="receiver" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ringLimiters" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ringLimiter" type="ringLimiter" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="stickMode" type="stickMode" minOccurs="0"/>
      <xs:element name="sicktrims" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="stickTrim" type="stickTrim" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="switches" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="switch" type="switch" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="telemetry" type="telemetry" minOccurs="0"/>
      <xs:element name="throttleSettings" type="throttleSettings" minOccurs="0"/>
      <xs:element name="trainerConfig" type="trainerConfig" minOccurs="0"/>
      <xs:element name="transmitterId" type="xs:long"/>
      <xs:element name="transmitterType" type="transmitterType" minOccurs="0"/>
      <xs:element name="vendor" type="vendor" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="channel">
    <xs:sequence>
      <xs:element name="center" type="xs:int"/>
      <xs:element name="failSafeMode" type="failSafeMode" minOccurs="0"/>
      <xs:element name="failSafePosition" type="xs:int"/>
      <xs:element name="function" type="function" minOccurs="0"/>
      <xs:element name="limitHigh" type="xs:int"/>
      <xs:element name="limitLow" type="xs:int"/>
      <xs:element name="mixOnly" type="xs:boolean"/>
      <xs:element name="phaseSettings" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="phaseSetting" type="channelPhaseSetting" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="reverse" type="xs:boolean"/>
      <xs:element name="trainerMode" type="trainerMode" minOccurs="0"/>
      <xs:element name="travelHigh" type="xs:int"/>
      <xs:element name="travelLow" type="xs:int"/>
      <xs:element name="virtual" type="xs:boolean"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="channelPhaseSetting">
    <xs:sequence/>
    <xs:attribute name="nonDelayed" type="xs:boolean" use="required"/>
    <xs:attribute name="phase" type="xs:IDREF"/>
  </xs:complexType>

  <xs:complexType name="phase" abstract="true">
    <xs:sequence>
      <xs:element name="channel1Curve" type="curve" minOccurs="0"/>
      <xs:element name="controls" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="control" type="control" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="dualRateExpos" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="dualRateExpo" type="dualRateExpo" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="motorOn" type="xs:boolean"/>
      <xs:element name="phaseName" type="xs:string" minOccurs="0"/>
      <xs:element name="phaseSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="phaseSwitchTime" type="xs:double"/>
      <xs:element name="phaseTimer" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="phaseType" type="phaseType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="curve">
    <xs:sequence>
      <xs:element name="points" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="point" type="curvePoint" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="smoothing" type="xs:boolean"/>
      <xs:element name="type" type="curveType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="curvePoint">
    <xs:sequence>
      <xs:element name="enabled" type="xs:boolean"/>
      <xs:element name="position" type="xs:int"/>
      <xs:element name="value" type="xs:int"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="control">
    <xs:sequence>
      <xs:element name="inputControl" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="mode" type="controlMode" minOccurs="0"/>
      <xs:element name="offset" type="xs:int"/>
      <xs:element name="timeHigh" type="xs:double"/>
      <xs:element name="timeLow" type="xs:double"/>
      <xs:element name="toggleHighSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="toggleLowSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="travelHigh" type="xs:int"/>
      <xs:element name="travelLow" type="xs:int"/>
      <xs:element name="trim" type="xs:int"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="switch">
    <xs:sequence>
      <xs:element name="assignment" type="switchAssignment" minOccurs="0"/>
      <xs:element name="direction" type="xs:int"/>
      <xs:element name="function" type="switchFunction" minOccurs="0"/>
      <xs:element name="number" type="xs:int"/>
      <xs:element name="qualifier" type="xs:anyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="type" type="switchType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="dualRateExpo">
    <xs:sequence>
      <xs:element name="dualRate" type="dualRate" minOccurs="0"/>
      <xs:element name="expo" type="expo" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="dualRate">
    <xs:sequence>
      <xs:element name="switch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="values" type="xs:int" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="function" type="function"/>
  </xs:complexType>

  <xs:complexType name="expo">
    <xs:sequence>
      <xs:element name="switch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="values" type="xs:int" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="function" type="function"/>
  </xs:complexType>

  <xs:complexType name="clock">
    <xs:sequence>
      <xs:element name="alarm" type="xs:int"/>
      <xs:element name="function" type="clockFunction" minOccurs="0"/>
      <xs:element name="minutes" type="xs:int"/>
      <xs:element name="mode" type="clockMode" minOccurs="0"/>
      <xs:element name="seconds" type="xs:int"/>
      <xs:element name="switch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="timer" type="xs:int"/>
      <xs:element name="timerMinutes" type="xs:int"/>
      <xs:element name="timerSeconds" type="xs:int"/>
      <xs:element name="type" type="clockType" minOccurs="0"/>
      <xs:element name="value" type="xs:int"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="wingedProfiTrim">
    <xs:sequence>
      <xs:element name="enabled" type="xs:boolean"/>
      <xs:element name="inputControl" type="xs:IDREF" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="channelMapping">
    <xs:sequence/>
    <xs:attribute name="inputChannel" type="xs:int" use="required"/>
    <xs:attribute name="outputChannel" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="channelSequencer">
    <xs:sequence>
      <xs:element name="maxStep" type="xs:int"/>
      <xs:element name="powerOffStatus" type="sequenceStatus" minOccurs="0"/>
      <xs:element name="sequences" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="sequence" type="sequence" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="stepTimes" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="stepTime" type="xs:double" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="switch" type="xs:IDREF" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="sequence">
    <xs:sequence>
      <xs:element name="enabled" type="xs:boolean"/>
      <xs:element name="outputChannel" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="stepPosition" type="xs:int" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="controlSwitch">
    <xs:complexContent>
      <xs:extension base="switch">
        <xs:sequence>
          <xs:element name="combineSwitch" type="xs:IDREF" minOccurs="0"/>
          <xs:element name="enabled" type="xs:boolean"/>
          <xs:element name="position" type="xs:int"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="dualMixer">
    <xs:sequence>
      <xs:element name="channel" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="diff" type="xs:int"/>
      <xs:element name="number" type="xs:int"/>
      <xs:element name="type" type="mixerType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="freeMixer" abstract="true">
    <xs:sequence>
      <xs:element name="fromChannel" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="inputType" type="mixerInputType" minOccurs="0"/>
      <xs:element name="phaseSettings" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="phaseSetting" type="freeMixerPhaseSetting" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="switch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="toChannel" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="type" type="mixerType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="freeMixerPhaseSetting">
    <xs:sequence/>
    <xs:attribute name="enabled" type="xs:boolean" use="required"/>
    <xs:attribute name="phase" type="xs:IDREF"/>
  </xs:complexType>

  <xs:complexType name="logicalSwitch">
    <xs:complexContent>
      <xs:extension base="switch">
        <xs:sequence>
          <xs:element name="enabled" type="xs:boolean"/>
          <xs:element name="mode" type="logicalSwitchMode" minOccurs="0"/>
          <xs:element name="switches" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="switch" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="hfModule">
    <xs:sequence>
      <xs:element name="type" type="hfModuleType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="mp3Player">
    <xs:sequence>
      <xs:element name="album" type="xs:int"/>
      <xs:element name="mode" type="mp3PlayerMode" minOccurs="0"/>
      <xs:element name="track" type="xs:int"/>
      <xs:element name="volume" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="multichannel">
    <xs:sequence>
      <xs:element name="controls" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="control" type="control" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="enabled" type="xs:boolean"/>
      <xs:element name="inputChannel" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="mode" type="multichannelMode" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="phaseAssignment">
    <xs:sequence>
      <xs:element name="assignments" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="assignment" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="combiCSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="combiDSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="combiESwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="combiFSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="priorityASwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="priorityBSwitch" type="xs:IDREF" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="receiver">
    <xs:sequence>
      <xs:element name="bound" type="xs:boolean"/>
      <xs:element name="channelMappings" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="channelMapping" type="channelMapping" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="rfid" type="xs:long"/>
      <xs:element name="telemetry" type="xs:boolean"/>
    </xs:sequence>
    <xs:attribute name="number" type="xs:ID"/>
  </xs:complexType>

  <xs:complexType name="ringLimiter">
    <xs:sequence>
      <xs:element name="enabled" type="xs:boolean"/>
      <xs:element name="inputChannels" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="inputChannel" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="limits" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="limit" type="xs:int" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="offsets" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="offset" type="xs:int" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="outputChannels" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="outputChannel" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="number" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="stickTrim">
    <xs:sequence>
      <xs:element name="increment" type="xs:int"/>
      <xs:element name="mode" type="trimMode" minOccurs="0"/>
      <xs:element name="timeHigh" type="xs:int"/>
      <xs:element name="timeLow" type="xs:int"/>
    </xs:sequence>
    <xs:attribute name="channel" type="xs:int" use="required"/>
  </xs:complexType>

  <xs:complexType name="telemetry">
    <xs:sequence>
      <xs:element name="currentSensor" type="sensorType" minOccurs="0"/>
      <xs:element name="currentSensorPage" type="xs:int"/>
      <xs:element name="selectedSensor" type="sensorType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="varioTone" type="switch" minOccurs="0"/>
      <xs:element name="voiceDelay" type="xs:int"/>
      <xs:element name="voiceRepeat" type="switch" minOccurs="0"/>
      <xs:element name="voiceTrigger" type="switch" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="throttleSettings">
    <xs:sequence>
      <xs:element name="throttleCutOf" type="throttleCutOf" minOccurs="0"/>
      <xs:element name="throttleLastIdlePosition" type="xs:int"/>
      <xs:element name="throttleTrim" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="throttleCutOf">
    <xs:sequence>
      <xs:element name="position" type="xs:int"/>
      <xs:element name="switch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="threshold" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="trainerConfig">
    <xs:sequence>
      <xs:element name="pupilChannel" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="pupilId" type="xs:long"/>
      <xs:element name="trainerChannel" type="xs:IDREF" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="trainerId" type="xs:long"/>
      <xs:element name="trainerSwitch" type="xs:IDREF" minOccurs="0"/>
      <xs:element name="wireless" type="xs:boolean"/>
    </xs:sequence>
  </xs:complexType>

