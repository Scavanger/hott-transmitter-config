<?xml version="1.0" encoding="utf-8"?>
<Composite xmlns="http://www.swtxml.com/swt" xmlns:on="http://www.swtxml.com/events"
	layout="layout:grid; numColumns:3" background="#FAF9B3">

	<Button text="Load MDL" style="PUSH, CENTER"
		layoutData="widthHint:180; heightHint:26; horizontalAlignment:CENTER; verticalAlignment:BEGINNING; grabExcessHorizontalSpace:true;"
		on:widgetSelected="onLoadMDL" />

	<Label id="versionLabel" style="CENTER" text="?" background="#FAF9B3"
		layoutData="heightHint:26; horizontalAlignment:CENTER; verticalAlignment:BEGINNING; grabExcessHorizontalSpace:true;" />

	<Button text="Save MDL" style="PUSH, CENTER"
		layoutData="widthHint:180; heightHint:26; horizontalAlignment:CENTER; verticalAlignment:BEGINNING; grabExcessHorizontalSpace:true;"
		on:widgetSelected="onSaveMDL" />

	<Browser id="browser" style="BORDER"
		layoutData="grabExcessHorizontalSpace:true; grabExcessVerticalSpace:true; horizontalAlignment:FILL; verticalAlignment:FILL; horizontalSpan:3;"
		 />
</Composite>